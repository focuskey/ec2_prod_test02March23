    />

    <menuitem id="menu_carpetcall_stage"
              name="Stages"
              action="action_carpetcall_stage"
              parent="menu_carpetcall_configuration"/>

    <menuitem id="menu_carpetcall_builder"
              name="Builder"
              action="action_carpetcall_builder_add"
              parent="menu_carpetcall_configuration"/>


