    <!-- Menu item to open the Book list -->
    <menuitem id="menu_carpetcall_carpet" name="Carpet" parent="menu_carpetcall" action="action_carpetcall_carpet"/>
    <menuitem id="menu_carpetcall_timber" name="Timber" parent="menu_carpetcall" action="action_carpetcall_timber"/>
    <menuitem id="menu_carpetcall_vinyl" name="Vinyl" parent="menu_carpetcall" action="action_carpetcall_vinyl"/>
    <menuitem id="menu_carpetcall_underlay" name="Underlay" parent="menu_carpetcall" action="action_carpetcall_underlay"/>

    <menuitem id="menu_carpetcall_underlay1" name="Underlay1" parent="menu_carpetcall_underlay" action="action_carpetcall_underlay1"/>
    <menuitem id="menu_carpetcall_underlay2" name="Underlay2" parent="menu_carpetcall_underlay" action="action_carpetcall_underlay2"/>
