<odoo>
    <!-- Carpetcall App Menu -->
    <menuitem id="menu_carpetcall" name="carpetCALL" />

    <!-- Action to open the Book list -->
    <record id="action_carpetcall_carpet" model="ir.actions.act_window">
        <field name="name">Carpet</field>
        <field name="res_model">carpetcall.book</field>
        <field name="view_mode">tree,form</field>
    </record>

    <record id="action_carpetcall_timber" model="ir.actions.act_window">
        <field name="name">Timber</field>
        <field name="res_model">carpetcall.book</field>
        <field name="view_mode">tree,form</field>
    </record>

    <record id="action_carpetcall_vinyl" model="ir.actions.act_window">
        <field name="name">Vinyl</field>
        <field name="res_model">carpetcall.book</field>
        <field name="view_mode">tree,form</field>
    </record>


    <record id="action_carpetcall_underlay" model="ir.actions.act_window">
        <field name="name">Underlay</field>
        <field name="res_model">carpetcall.book</field>
        <field name="view_mode">tree,form</field>
    </record>

    <record id="action_carpetcall_underlay1" model="ir.actions.act_window">
        <field name="name">Underlay_test1</field>
        <field name="res_model">carpetcall.book</field>
        <field name="view_mode">tree,form</field>
    </record>

    <record id="action_carpetcall_underlay2" model="ir.actions.act_window">
        <field name="name">Underlay_test2</field>
        <field name="res_model">carpetcall_franchise.testsql</field>
        <field name="view_mode">tree</field>
    </record>

    <record id="action_carpetcall_underlay3" model="ir.actions.act_window">
        <field name="name">Underlay_test3</field>
        <field name="res_model">carpetcall_franchise.partner_product</field>
        <field name="view_mode">tree</field>
    </record>

    <record id="action_carpetcall_user_lay" model="ir.actions.act_window">
        <field name="name">UserLayer_vie323w</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">carpetcall.book</field>
        <field name="view_mode">tree</field>
        <field name="context">{}</field>
        <field name="domain">[('retail_price', '>', '400')]</field>
<!--            <field name="domain">[('create_uid', '=', '2')]</field>-->
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                Create your first carpetCALL order!
            </p>
        </field>
    </record>


    <record model="ir.actions.act_window" id="action_carpetcall_underlay4">
            <field name="name">Product Partner Relation</field>
            <field name="res_model">carpetcall_franchise.partner_product</field>
            <field name="view_mode">tree</field>
    </record>

    <record id="action_carpetcall_stage" model="ir.actions.act_window">
        <field name="name">Stage45s</field>
        <field name="res_model">carpetcall.book.stage</field>
        <field name="view_mode">tree,form</field>
    </record>

    <record id="action_carpetcall_builder_add" model="ir.actions.act_window">
        <field name="name">Builder</field>
        <field name="res_model">carpetcall.builder</field>
        <field name="view_mode">tree,form</field>
    </record>

